journal = {Eur. J. Protistol.},
keywords = {Axenic culture,DGGE,Endosymbiont,Nitrate utilization,Paramecium bursaria},
pages = {193--202},
title = {{Establishment of axenic endosymbiotic strains of Japanese Paramecium bursaria and the utilization of carbohydrate and nitrogen compounds by the isolated algae}},
url = {http://www.sciencedirect.com/science/article/pii/S0932473905000283},
