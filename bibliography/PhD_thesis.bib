file = {:home/fin/Documents/papers/mendeley/Jeanfils, Canisius, Burlion\_1993\_Effect of high nitrate concentrations on growth and nitrate uptake by free-living and immobilized Chlor.pdf:pdf},
issn = {0921-8971},
