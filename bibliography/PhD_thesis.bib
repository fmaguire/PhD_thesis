journal = {Protist},
keywords = {Chlorella,Chlorella: physiology,Chlorella: ultrastructure,Intracellular Membranes,Intracellular Membranes: ultrastructure,Paramecium,Paramecium: microbiology,Paramecium: physiology,Paramecium: ultrastructure,Symbiosis,Vacuoles,Vacuoles: microbiology,Vacuoles: physiology,Vacuoles: ultrastructure},
month = jul,
number = {4},
pages = {658--70},
