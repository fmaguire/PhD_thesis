doi = {10.1371/journal.pone.0103833},
file = {:home/fin/Documents/papers/mendeley/Suzuki et al.\_2014\_GHOSTX an improved sequence homology search algorithm using a query suffix array and a database suffix array.\_PloS on.pdf:pdf},
issn = {1932-6203},
journal = {PLoS One},
number = {8},
