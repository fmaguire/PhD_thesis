@article{Phillips1980,
abstract = {Leguminous crops have played a crucial role in agricultural production throughout history. Their success in N-deficient soils results from root nodules containing symbiotic Rhizobium bacteria that reduce N2 to NH3. Chemical synthesis of alternative N fertilizers requires fossil fuel, but$\backslash$nRhizobium uses solar energy collected by plant photosynthesis. Increased awareness of the importance of Rhizobiurn-legume symbioses in many agricultural and marginal land environments has prompted scientists from diverse disciplines to consider enhancing the efficiency of symbiotic N2 fixation in legumes.$\backslash$nEfficiency in biological systems is the effectiveness with which energy is used for a particular process. Energy efficiency is calculated by relating energy use to minimum energy cost under defined conditions. Information summarized previously on the energy costs of symbiotic N2 fixation at the cellular (9, 22, 31, 96) and whole-plant (7, 93) levels is used herein as a basis$\backslash$nfor discussing enhancement of the efficiency of symbiotic N2 fixation in agronomic communities of legumes.},
author = {Phillips, D a},
doi = {10.1146/annurev.pp.31.060180.000333},
file = {:home/fin/Documents/papers/mendeley/Phillips\_1980\_Efficiency of Symbiotic Nitrogen Fixation in Legumes\_Annual Review of Plant Physiology.pdf:pdf},
