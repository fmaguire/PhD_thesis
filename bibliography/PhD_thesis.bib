issn = {1226-8372},
journal = {Biotechnol. Bioprocess Eng.},
keywords = {fatty acids,lipid,microalgae,municipal wastewater,nutrient,removal},
pages = {510--518},
title = {{Cultivation of a new microalga, Micractinium reisseri, in municipal wastewater for nutrient removal, biomass, lipid, and fatty acid production}},
