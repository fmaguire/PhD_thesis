journal = {Heredity (Edinb).},
keywords = {Animals,DNA Transposable Elements,Host-Parasite Interactions,Host-Parasite Interactions: genetics,RNA Interference,Viruses,Viruses: genetics,Viruses: pathogenicity},
pages = {195--202},
