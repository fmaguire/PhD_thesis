keywords = {Antarctic Regions,Biodiversity,DNA, Plant,DNA, Plant: genetics,Ecosystem,Eukaryota,Eukaryota: genetics,Eukaryota: physiology,Evolution, Molecular,Ice Cover,Phylogeny},
month = oct,
number = {1673},
pages = {3591--9},
pmid = {19625320},
title = {{Hidden levels of phylodiversity in Antarctic green algae: further evidence for the existence of glacial refugia.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2817313\&tool=pmcentrez\&rendertype=abstract},
