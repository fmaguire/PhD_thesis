doi = {10.1145/1389095.1389254},
file = {:home/fin/Documents/papers/mendeley/Wolpert, Macready\_1995\_No free lunch theorems for search\_Technical Report SFI-TR-95-02-010.pdf:pdf},
isbn = {9781605581309},
