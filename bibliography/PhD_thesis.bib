isbn = {1471-2164 (Electronic)$\backslash$n1471-2164 (Linking)},
issn = {1471-2164},
journal = {BMC Genomics},
keywords = {Algorithms,Bayes Theorem,Cluster Analysis,Escherichia coli,Escherichia coli: genetics,Sequence Analysis, DNA,Single-Cell Analysis},
number = {Suppl 1},
pages = {S7},
pmid = {23368723},
