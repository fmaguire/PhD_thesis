journal = {Eur. J. Protistol.},
keywords = {Chlorella,Chlorella: physiology,Holosporaceae,Holosporaceae: physiology,Host-Pathogen Interactions,Paramecium,Paramecium: microbiology,Symbiosis},
month = may,
