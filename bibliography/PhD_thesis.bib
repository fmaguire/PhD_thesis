doi = {10.1105/tpc.110.076406},
file = {:home/fin/Documents/papers/mendeley/Blanc et al.\_2010\_The Chlorella variabilis NC64A genome reveals adaptation to photosymbiosis, coevolution with viruses, and cryptic s(2).pdf:pdf},
