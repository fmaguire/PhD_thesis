issn = {14665026},
journal = {Int. J. Syst. Evol. Microbiol.},
pages = {2535--2540},
pmid = {17082386},
title = {{Candidatus Midichloria mitochondrii', an endosymbiont of the Ixodes ricinus with a unique intramitochondrial lifestyle}},
volume = {56},
year = {2006}
}
