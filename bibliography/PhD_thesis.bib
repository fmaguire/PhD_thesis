@article{Wootton2007,
abstract = {Planktonic flagellates and ciliates are the major consumers of phytoplankton and bacterioplankton in aquatic environments, playing a pivotal role in carbon cycling and nutrient regeneration. Despite certain unicellular predators using chemosensory responses to locate and select their prey, the biochemical mechanisms behind prey reception and selection have not been elucidated. Here we identify a Ca(2+)-dependent, mannose-binding lectin on the marine dinoflagellate Oxyrrhis marina, which is used as a feeding receptor for recognizing prey. Blocking the receptor using 20 microM mannose-BSA inhibited ingestion of phytoplankton prey, Isochrysis galbana, by 60\%. In prey selection studies, O. marina ingested twice as many 6 mum diameter beads coated with mannose-BSA as those coated with galNac-BSA. When pre-incubated with mannose-BSA, O. marina was no longer able to discriminate between different sugar-coated beads. Thus, these findings reveal molecular mechanisms of protozoan prey recognition. Our results also indicate the functional similarity between cellular recognition used by planktonic protozoa to discriminate between different prey items, and those used by metazoan phagocytic blood cells to recognize invading microorganisms.},
author = {Wootton, Emma C and Zubkov, Mikhail V and Jones, D Hugh and Jones, Ruth H and Martel, Claire M and Thornton, Catherine a and Roberts, Emily C},
doi = {10.1111/j.1462-2920.2006.01130.x},
file = {:home/fin/Documents/papers/mendeley/Wootton et al.\_2007\_Biochemical prey recognition by planktonic protozoa.\_Environmental microbiology.pdf:pdf},
issn = {1462-2912},
journal = {Environ. Microbiol.},
keywords = {Animals,Calcium,Calcium: metabolism,Dinoflagellida,Dinoflagellida: chemistry,Dinoflagellida: cytology,Dinoflagellida: metabolism,Mannose-Binding Lectins,Mannose-Binding Lectins: metabolism,Phagocytosis,Phytoplankton,Phytoplankton: metabolism,Zooplankton,Zooplankton: chemistry,Zooplankton: cytology,Zooplankton: metabolism},
month = jan,
