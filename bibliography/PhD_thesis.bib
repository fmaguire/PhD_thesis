journal = {Environ. Microbiol.},
keywords = {Animals,Archaea,Archaea: physiology,Bacterial,Bacterial Physiological Phenomena,Biological Evolution,Chlorophyta,Chlorophyta: genetics,Chlorophyta: physiology,Chlorophyta: ultrastructure,Chloroplasts,Chloroplasts: physiology,Gene Transfer,Genes,Horizontal,Invertebrates,Invertebrates: genetics,Invertebrates: microbiology,Invertebrates: physiology,Mitochondria,Mitochondria: physiology,Symbiosis},
pages = {641--9},
