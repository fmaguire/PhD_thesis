journal = {Chem. Sci.},
pages = {3135},
title = {{NanoSIMS combined with fluorescence microscopy as a tool for subcellular imaging of isotopically labeled platinum-based anticancer drugs}},
url = {http://xlink.rsc.org/?DOI=c3sc53426j},
volume = {5},
year = {2014}
}
