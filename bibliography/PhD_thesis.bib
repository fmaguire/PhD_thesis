@article{Kadono2004,
abstract = {Paramecium bursaria (green paramecium) possesses endosymbiotically growing chlorella-like green algae. An aposymbiotic cell line of P. bursaria (MBw-1) was prepared from the green MB-1 strain with the herbicide paraquat. The SA-2 clone of symbiotic algae was employed to reinfect MBw-1 cells and thus a regreened cell line (MBr-1) was obtained. The regreened paramecia were used to study the impact of the host's growth status on the life cycle of the symbiotic algae. Firstly, the relationship between the timing of algal propagation and the host cell division was investigated by counting the algal cells in single host cells during and after the host cell division and also in the stationary phase. Secondly, the changes in the endogenous chlorophyll level, DNA content, and cell size in the symbiotic algae were monitored by flow cytometry and fluorescence microscopy. The number of algae was shown to be doubled prior to or during the host cell division and the algal population in the two daughter cells is maintained at constant level until the host cell cycle reenters the cytokinesis, suggesting that algal propagation and cell cycle are dependent on the host's cell cycle. During the host's stationary growth, unicellular algal vegetatives with low chlorophyll content were dominant. In contrast, complexes of algal cells called sporangia (containing 1-4 autospores) were present in the logarithmically growing hosts, indicating that algal cell division leading to the formation of sporangia with multiple autospores is active in the dividing paramecia.},
author = {Kadono, T and Kawano, T and Hosoya, H and Kosaka, T},
doi = {10.1007/s00709-004-0046-6},
file = {:home/fin/Documents/papers/mendeley/Kadono et al.\_2004\_Flow cytometric studies of the host-regulated cell cycle in algae symbiotic with green paramecium.\_Protoplasma.pdf:pdf},
issn = {0033-183X},
journal = {Protoplasma},
keywords = {Animals,Cell Cycle,Cell Division,Cell Size,Chlorophyll,Chlorophyll: metabolism,DNA,DNA: metabolism,Eukaryota,Eukaryota: cytology,Eukaryota: genetics,Eukaryota: physiology,Flow Cytometry,Life Cycle Stages,Paramecium,Paramecium: growth \& development,Paramecium: microbiology,Symbiosis},
month = jun,
pages = {133--141},
pmid = {15221518},
title = {{Flow cytometric studies of the host-regulated cell cycle in algae symbiotic with green paramecium.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15221518},
volume = {223},
year = {2004}
}
@article{Sebe-Pedros2013,
abstract = {The evolution of metazoans from their unicellular ancestors was one of the most important events in the history of life. However, the cellular and genetic changes that ultimately led to the evolution of multicellularity are not known. In this study, we describe an aggregative multicellular stage in the protist Capsaspora owczarzaki, a close unicellular relative of metazoans. Remarkably, transition to the aggregative stage is associated with significant upregulation of orthologs of genes known to establish multicellularity and tissue architecture in metazoans. We further observe transitions in regulated alternative splicing during the C. owczarzaki life cycle, including the deployment of an exon network associated with signaling, a feature of splicing regulation so far only observed in metazoans. Our results reveal the existence of a highly regulated aggregative stage in C. owczarzaki and further suggest that features of aggregative behavior in an ancestral protist may had been co-opted to develop some multicellular properties currently seen in metazoans. DOI: http://dx.doi.org/10.7554/eLife.01287.001.},
author = {Seb\'{e}-Pedr\'{o}s, Arnau and Irimia, Manuel and del Campo, Javier and Parra-Acero, Helena and Russ, Carsten and Nusbaum, Chad and Blencowe, Benjamin J. and Ruiz-Trillo, I\~{n}aki},
doi = {10.7554/eLife.01287},
file = {:home/fin/Documents/papers/mendeley/Seb\'{e}-Pedr\'{o}s et al.\_2013\_Regulated aggregative multicellularity in a close unicellular relative of metazoa\_eLife.pdf:pdf},
isbn = {10.7554/eLife.01287},
issn = {2050084X},
journal = {Elife},
pages = {1--20},
pmid = {24368732},
title = {{Regulated aggregative multicellularity in a close unicellular relative of metazoa}},
volume = {2013},
year = {2013}
}
