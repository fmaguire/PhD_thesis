doi = {10.1093/bioinformatics/btu170},
file = {:home/fin/Documents/papers/mendeley/Bolger, Lohse, Usadel\_2014\_Trimmomatic A Flexible Trimmer for Illumina Sequence Data\_Bioinformatics.pdf:pdf},
isbn = {1367-4803},
