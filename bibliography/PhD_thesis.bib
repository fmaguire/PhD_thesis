file = {:home/fin/Documents/papers/mendeley/Kodama et al.\_2014\_Comparison of gene expression of Paramecium bursaria with and without Chlorella variabilis symbionts.\_BMC genomics.pdf:pdf},
isbn = {1471216415183},
issn = {1471-2164},
journal = {BMC Genomics},
keywords = {chlorella variabilis,paramecium bursaria,secondary symbiosis,transcriptome analysis},
pages = {183},
pmid = {24612690},
title = {{Comparison of gene expression of Paramecium bursaria with and without Chlorella variabilis symbionts.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4029085\&tool=pmcentrez\&rendertype=abstract},
