issn = {1362-4962},
journal = {Nucleic Acids Res.},
keywords = {Algorithms,Gene Expression,Internet,Protein Interaction Maps,Proteins,Proteins: chemistry,Proteins: genetics,Proteins: physiology,Sequence Analysis, Protein,Software},
