@incollection{AubussonFleury2015,
abstract = {Abstract Paramecium is a single cell able to divide in its morphologically differentiated stage that has many cilia anchored at its cell surface. Many thousands of cilia are thus assembled in a short period of time during division to duplicate the cell pattern while the cell continues swimming. Most, but not all, of these sensory cilia are motile and involved in two main functions: prey capture and cell locomotion. These cilia display heterogeneity, both in their length and their biochemical properties. Thanks to these properties, as well as to the availability of many postgenomic tools and the possibility to follow the regrowth of cilia after deciliation, Paramecium offers a nice opportunity to study the assembly of the cilia, as well as the genesis of their diversity within a single cell. In this paper, after a brief survey of Paramecium morphology and cilia properties, we describe the tools and the protocols currently used for immunofluorescence, transmission electron microscopy, and ultrastructural immunocytochemistry to analyze cilia, with special recommendations to overcome the problem raised by cilium diversity.},
author = {Aubusson-Fleury, Anne and Cohen, Jean and Lemullois, Michel},
booktitle = {Methods in Cilia \& Flagella},
doi = {http://dx.doi.org/10.1016/bs.mcb.2014.12.007},
editor = {Basto, Renata and Marshall, Wallace F},
issn = {0091-679X},
keywords = {Basal body,Ciliates,Ciliogenesis,Cilium,Deciliation,Transition zone,Tubulin posttranslational modification},
pages = {457--485},
publisher = {Academic Press},
series = {Methods in Cell Biology},
title = {{Chapter 22 - Ciliary heterogeneity within a single cell: The Paramecium model}},
url = {http://www.sciencedirect.com/science/article/pii/S0091679X14000521},
volume = {127},
year = {2015}
}
