file = {:home/fin/Documents/papers/mendeley/Rees et al.\_1995\_In Situ Glutamine Synthetase Activity in a Marine Unicellular Alga (Development of a Sensitive Colorimetric Assay and t.pdf:pdf},
isbn = {1532-2548 (Electronic)$\backslash$r0032-0889 (Linking)},
issn = {1532-2548},
journal = {Plant Physiol.},
number = {1 995},
pages = {1405--1410},
pmid = {12228676},
title = {{In Situ Glutamine Synthetase Activity in a Marine Unicellular Alga (Development of a Sensitive Colorimetric Assay and the Effects of Nitrogen Status on Enzyme Activity).}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=157675\&tool=pmcentrez\&rendertype=abstract},
