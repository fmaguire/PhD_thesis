doi = {10.7717/peerj.113},
eprint = {1304.0817},
file = {:home/fin/Documents/papers/mendeley/Macmanes, Eisen\_2013\_Improving transcriptome assembly through error correction of high-throughput sequence reads.\_PeerJ.pdf:pdf},
isbn = {2167-8359 (Electronic)},
issn = {2167-8359},
journal = {PeerJ},
pages = {e113},
pmid = {23904992},
title = {{Improving transcriptome assembly through error correction of high-throughput sequence reads.}},
