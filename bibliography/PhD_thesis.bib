doi = {10.1038/nprot.2014.006},
file = {:home/fin/Documents/papers/mendeley/Picelli et al.\_2014\_Full-length RNA-seq from single cells using Smart-seq2.\_Nature protocols.pdf:pdf},
isbn = {doi:10.1038/nprot.2014.006},
issn = {1750-2799},
journal = {Nat. Protoc.},
number = {1},
pages = {171--81},
pmid = {24385147},
title = {{Full-length RNA-seq from single cells using Smart-seq2.}},
