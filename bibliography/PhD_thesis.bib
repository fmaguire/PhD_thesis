journal = {Nature},
keywords = {Animals,Duplicate,Duplicate: genetics,Eukaryotic Cells,Eukaryotic Cells: metabolism,Evolution,Gene Duplication,Genes,Genome,Genomics,Molecular,Molecular Sequence Data,Paramecium tetraurelia,Paramecium tetraurelia: genetics,Phylogeny,Protozoan,Protozoan: genetics},
