journal = {Mol. Ecol. Resour.},
keywords = {2 $\backslash$n                        nd and 3,454,Helicos,Illumina,Ion Torrent,Life technologies,Massively parallel sequencing,Pacific biosystems,Roche,SOLiD},
pages = {759--769},
pmid = {21592312},
title = {{Field guide to next-generation DNA sequencers}},
volume = {11},
year = {2011}
