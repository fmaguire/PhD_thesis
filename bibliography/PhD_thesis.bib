journal = {Nat. Rev. Microbiol.},
keywords = {Archaea,Archaea: classification,Archaea: genetics,Bacteria,Bacteria: classification,Bacteria: genetics,Biological Evolution,Classification,Classification: methods,Eukaryota,Eukaryota: classification,Eukaryota: genetics,Genome,Genome: genetics},
